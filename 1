function newUser(){
    let john = {name:"John",surname:"Smith",id:1};
    let mary = {name:"Mary",surname:"Key",id:2};
    let users = [john,mary];
    let newUsers = users.map(function(user){
        return {
          fullName: `${user.name} ${user.surname}`,
          id: user.id
        }
    });
    return newUsers;
}
